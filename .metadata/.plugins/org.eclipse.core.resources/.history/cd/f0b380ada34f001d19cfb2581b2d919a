package br.edu.infnet.mypet.model.test;

import org.springframework.boot.ApplicationArguments;
import org.springframework.boot.ApplicationRunner;
import org.springframework.stereotype.Component;

import br.edu.infnet.mypet.model.domain.Banho;

@Component
public class BanhoTeste implements ApplicationRunner {

	@Override
	public void run(ApplicationArguments args) throws Exception {
		Banho b1 = new Banho();
		b1.setComShampoo(true);
		b1.setPorteAnimal("Médio");
		b1.setPreco(40.0);
		
		Banho b2 = new Banho();
		b2.setComShampoo(true);
		b2.setPorteAnimal("Médio");
		b2.setPreco(40.0);
		
		Banho b3 = new Banho();
		b3.setComShampoo(true);
		b3.setPorteAnimal("Médio");
		b3.setPreco(40.0);
	}

}
